{
  "presets": [
    "@babel/flow",
    "@babel/env",
    "@babel/react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "contracts": "./src/contracts",
          "components": "./src/components",
          "types": "./src/types",
          "reducers": "./src/reducers",
          "actions": "./src/actions",
          "hooks": "./src/hooks",
          "constants": "./src/constants",
          "db": "./src/db",
          "styles": "./src/styles",
          "images": "./src/images",
          "fragments": "./src/fragments",
          "src": "./src",
          "extensions": [".js", ".jsx", ".json"]
        }
      }
    ]
  ]
}
